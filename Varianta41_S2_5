//Programul afiseaza matricea obtinuta in urma adunarii indicilor i si j al fiecarui element.

#include <iostream>
using namespace std;
int a[25][25], i, j, n;
int main()
{
    cin >> n;
    for (i = 1; i <= n; i++)
        for (j = 1; j <= n; j++)
            a[i][j] = i + j;
    for (i = 1; i <= n; i++)
    {
        for (j = 1; j <= n; j++)
            cout << a[i][j] << ' ';
        cout << endl;
    }
    return 0;
}
