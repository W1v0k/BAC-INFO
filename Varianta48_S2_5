#include <iostream>
using namespace std;
int n, a[10][10], k=2, i, j;
int main()
{
    cin >> n;
    for (i = 1; i <= n; i++)
        for (j = 1; j <= n; j++)
        {a[i][j] = k;
    k += 2;
}

            for (i = 1; i <= n; i++) {
                for (j = 1; j <= n; j++)
                    cout << a[i][j] << ' ';
                cout << endl;
            }
    return 0;
}
